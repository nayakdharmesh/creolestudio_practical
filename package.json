{
  "name": "onclick_project",
  "version": "1.0.0",
  "description": "onclick practical.",
  "main": "server.js",
  "scripts": {
    "tsc": "tsc",
    "lint": "tslint -c tslint.json 'src/**/*.ts' --fix",
    "start": "tsc --watch & nodemon --inspect dist",
    "build": "tsc && cp -R src/templates/* dist/templates",
    "build-dev": "tsc && rm -rf dist/templates && mkdir -p dist/templates && cp -R src/templates/* dist/templates",
    "build-stage": "tsc && rm -rf dist/templates && mkdir -p dist/templates && cp -R src/templates/* dist/templates",
    "build-prod": "tsc && mkdir -p dist/templates && cp -R src/templates/* dist/templates",
    "env": "sync-dotenv --sample env.sample"
  },
  "author": "Dharmesh Nayak",
  "license": "ISC",
  "dependencies": {
    "@types/lodash": "^4.14.119",
    "bcryptjs": "^2.4.3",
    "class-validator": "^0.9.1",
    "compression": "^1.7.3",
    "dotenv": "^6.2.0",
    "enum": "^2.5.0",
    "express": "^4.16.4",
    "helmet": "^3.15.0",
    "jm-ez-l10n": "^1.0.0",
    "jm-ez-mysql": "^2.3.0",
    "jsonwebtoken": "^8.4.0",
    "method-override": "^3.0.0",
    "moment-timezone": "^0.5.25",
    "morgan": "^1.9.1",
    "multer": "^1.4.4",
    "mysql": "^2.18.1",
    "path": "^0.12.7",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.17.0",
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.12",
    "@types/method-override": "0.0.31",
    "@types/node": "^10.12.21",
    "@types/node-uuid": "0.0.28",
    "@types/q": "^1.5.2",
    "@types/tedious": "^4.0.3",
    "husky": "^1.3.1",
    "rimraf": "^2.6.3",
    "ts-node": "^8.0.3",
    "tslint": "^5.12.1",
    "typescript": "^3.9.9",
    "typescript-eslint-parser": "^21.0.2"
  }
}
